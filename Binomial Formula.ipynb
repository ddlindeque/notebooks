    "def printPascals(n):\n",
    "    for row in range(1, n + 1):\n",
    "        val = 1\n",
    "        for col in range(1, row + 1):\n",
    "            print(f'{val} ', end='')\n",
    "            val = int(val * (row - col) / col)\n",
    "        print('')\n",
    "\n",
    "printPascals(5)"
